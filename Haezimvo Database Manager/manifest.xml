<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>9072a3ec-f857-43d9-a069-4c3c7fb6c522</Id>
  <Version>2.0.0.0</Version>
  <ProviderName>G Dave Lee</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Haezimvo Database Manager"/>
  <Description DefaultValue="A DB Manager for Haezimvo.xlsx"/>
  <IconUrl DefaultValue="https://localhost:3000/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://www.example.com"/>
  <AppDomains>
    <AppDomain>https://www.example.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000/taskpane/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="ApplyButton">
                  <Label resid="ApplyButton.Label"/>
                  <Supertip>
                    <Title resid="ApplyButton.Label"/>
                    <Description resid="ApplyButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>apply</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="BringButton">
                  <Label resid="BringButton.Label"/>
                  <Supertip>
                    <Title resid="BringButton.Label"/>
                    <Description resid="BringButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>bring</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="CombineButton">
                  <Label resid="CombineButton.Label"/>
                  <Supertip>
                    <Title resid="CombineButton.Label"/>
                    <Description resid="CombineButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>combine</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="DeliverButton">
                  <Label resid="DeliverButton.Label"/>
                  <Supertip>
                    <Title resid="DeliverButton.Label"/>
                    <Description resid="DeliverButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>deliver</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="ExploreButton">
                  <Label resid="ExploreButton.Label"/>
                  <Supertip>
                    <Title resid="ExploreButton.Label"/>
                    <Description resid="ExploreButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>explore</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="FixButton">
                  <Label resid="FixButton.Label"/>
                  <Supertip>
                    <Title resid="FixButton.Label"/>
                    <Description resid="FixButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>fix</FunctionName>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://localhost:3000/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://localhost:3000/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://localhost:3000/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="https://localhost:3000/commands/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://localhost:3000/taskpane/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with your sample add-in!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Database Manager"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Show Task Pane"/>
        <bt:String id="ApplyButton.Label" DefaultValue="Apply"/>
        <bt:String id="BringButton.Label" DefaultValue="Bring"/>
        <bt:String id="CombineButton.Label" DefaultValue="Combine"/>
        <bt:String id="DeliverButton.Label" DefaultValue="Deliver"/>
        <bt:String id="ExploreButton.Label" DefaultValue="Explore"/>
        <bt:String id="FixButton.Label" DefaultValue="Fix"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your sample add-in loaded successfully. Go to the HOME tab and click the 'Show Task Pane' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Show a Taskpane"/>
        <bt:String id="ApplyButton.Tooltip" DefaultValue="Apply changes to the workbook."/>
        <bt:String id="BringButton.Tooltip" DefaultValue="Bring reference data into the sheet."/>
        <bt:String id="CombineButton.Tooltip" DefaultValue="Combine and merge data."/>
        <bt:String id="DeliverButton.Tooltip" DefaultValue="Deliver the data to the output."/>
        <bt:String id="ExploreButton.Tooltip" DefaultValue="Explore and search data."/>
        <bt:String id="FixButton.Tooltip" DefaultValue="Fix data from logs."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>